<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
		.bigger {
			font-size: 200%;
		}
		
		.important {
			font-weight: bold;
		}
	</ui:style>
	<g:HorizontalPanel>
		<g:VerticalPanel>
			<g:Label addStyleNames="{style.bigger} {style.important}">tightie - we are evil.</g:Label>
			<g:Label addStyleNames="{style.important}">tightness index</g:Label>
			<g:FlexTable ui:field="grid" />
			<g:Label ui:field="log">Minimum roundtrip time: </g:Label>
		</g:VerticalPanel>
		<g:VerticalPanel>
			<g:Label addStyleNames="{style.important}">uncool sites - sites that grew too fast</g:Label>
			<!-- g:FlexTable ui:field="grid" / -->
		</g:VerticalPanel>
	</g:HorizontalPanel>
</ui:UiBinder> 